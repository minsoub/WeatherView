<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hist.weatherview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AirportActivity" />
        <activity android:name=".AirportDetailActivity" />
        <activity android:name=".thunderstroke.ThunderStrokeActivity" />
        <activity android:name=".thunderstroke.ThunderStrokeDetailActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".weatherlife.main.base.WeatherLifeActivity" />
        <activity android:name=".weatherlife.area.base.WeatherLifeAreaActivity"></activity>
        <activity
            android:theme="@style/AppTheme2"
            android:name=".weeklyweather.main.base.WeeklyWeatherActivity"

            />
        <activity
            android:theme="@style/AppTheme2"
            android:name=".weeklyweather.area.base.WeeklyWeatherAreaActivity"


            />
    </application>

</manifest>